# Declaración de variables básicas
int fa, a 
int y
char z = 2
bool b = true
float alfa = cos 10
# Declaración de vector
int[3] vec
vec[0] = 1
vec[1] = 2
vec[2] = 3

int vector_lenght = vec.len

# Declaración de tipo de registro

type Persona: 
{
    int edad, altura
    char inicial 
}
Persona p
p.edad = 2

# Declaración de variable de tipo registro
Persona[2] per
per[0].edad = 25


